 if choice == "1":
            patient_id = input("Enter Patient ID: ")
            detector.run_test(patient_id)

        elif choice == "2":
            detector.view_results()

        elif choice == "3":
            print("Exiting H. pylori detector. Goodbye!")
            break

        else:
            print("Invalid choice. Please try again.")

if __name__ == "__main__":
    main()
